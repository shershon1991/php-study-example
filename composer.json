  "autoload-dev": {
    "psr-4": {
      "Self\\Test\\": "tests"
    }
  },
  "config": {
    "secure-http": false
  },
  "require": {
    "monolog/monolog": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "case": "phpunit --bootstrap tests/bootstrap.php tests/Cases/LogTest.php"
  }
}
